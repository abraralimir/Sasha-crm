
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are completely public. 
    // Any user can read, write, and delete any data.
    // This is for development purposes only and must be secured before production.
    match /{path=**} {
      allow read, write: if true;
    }
  }
}
